{
  "name": "@trejgun/ssh-action",
  "version": "0.0.1",
  "description": "Run commands on a remote server via SSH.",
  "repository": {
    "type": "git",
    "url": "https://github.com/trejgun/ssh-action"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "pack": "ncc build"
  },
  "bin": {
    "github-action-ssh": "dist/index.js"
  },
  "preferGlobal": false,
  "keywords": [
    "typescript",
    "github",
    "github-actions",
    "actions",
    "ssh"
  ],
  "author": "trejgun",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "node-ssh": "^8.0.0",
    "ssh-exec": "2.0.0"
  },
  "devDependencies": {
    "@types/node": "12.19.7",
    "@zeit/ncc": "0.22.3",
    "prettier": "1.19.1",
    "typescript": "3.9.7"
  }
}
